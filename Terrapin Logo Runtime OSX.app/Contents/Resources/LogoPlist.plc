/* */// Change this to your bundle identifier string#define kBundleIdentifier "com.Terrapin.Logo2000.Carbon"// Change this to your short version string#define kBundleShortVersion __OUTPUT_FILENAME__ " version 3.0"// Change this to the string to display in the Finder's Get Info window#define kBundleGetInfo kBundleShortVersion ", Copyright © 2006 by Terrapin Software.  All rights reserved."plist{	dictionary	{		key "CFBundleIdentifier" value string kBundleIdentifier				key "CFBundleName" value string __OUTPUT_FILENAME__		key "CFBundleGetInfoString" value string kBundleGetInfo		key "CFBundleShortVersionString" value string kBundleShortVersion		key "CFBundleIconFile" value string __OUTPUT_FILENAME__ ".rsrc"		key "CFBundlePackageType" value string __OUTPUT_TYPE__		key "CFBundleSignature" value string __OUTPUT_CREATOR__		key "CFBundleExecutable" value string __OUTPUT_FILENAME__		key "CFBundleVersion" value string "2.4"				key "CFBundleDevelopmentRegion" value string "English"		key "CFBundleInfoDictionaryVersion" value string "6.0"		key "CFBundleDocumentTypes" value array		[			dictionary			{				key "CFBundleTypeExtensions" value array [ string "lgo" ]//				key "CFBundleTypeIconFile" value string __OUTPUT_FILENAME__ ".rsrc"				key "CFBundleTypeName" value string "Terrapin Logo program"				key "CFBundleTypeOSTypes" value array [ string "TEXT" ]				key "CFBundleTypeRole" value string "Editor"			}			dictionary			{				key "CFBundleTypeExtensions" value array [ string "lgz" ]//				key "CFBundleTypeIconFile" value string __OUTPUT_FILENAME__ ".rsrc"				key "CFBundleTypeName" value string "Terrapin Logo archive"				key "CFBundleTypeOSTypes" value array [ string "Data" ]				key "CFBundleTypeRole" value string "Editor"			}			dictionary			{				key "CFBundleTypeExtensions" value array [ string "pict" ]				key "CFBundleTypeName" value string "Pict Image file"				key "CFBundleTypeOSTypes" value array [ string "PICT" ]				key "CFBundleTypeRole" value string "Editor"			}			dictionary			{				key "CFBundleTypeExtensions" value array [ string "pref" ]				key "CFBundleTypeName" value string "Preferences file"				key "CFBundleTypeOSTypes" value array [ string "PREF" ]				key "CFBundleTypeRole" value string "Editor"			}			dictionary			{				key "CFBundleTypeExtensions" value array [ string "jpg" ]				key "CFBundleTypeName" value string "JPEG Image file"				key "CFBundleTypeOSTypes" value array [ string "JPEG" ]				key "CFBundleTypeRole" value string "Editor"			}			dictionary			{				key "CFBundleTypeExtensions" value array [ string "bmp" ]				key "CFBundleTypeName" value string "BMP Image file"				key "CFBundleTypeOSTypes" value array [ string "BMPp" string "BMP " ]				key "CFBundleTypeRole" value string "Editor"			}		]	}}// Change this to be localized for Englishlocalize "English"{	comment "Localized versions of Info.plist keys"	key "CFBundleName" value string __OUTPUT_FILENAME__	key "CFBundleGetInfoString" value string kBundleGetInfo	key "CFBundleShortVersionString" value string kBundleShortVersion}