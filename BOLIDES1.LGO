TO FALL
	WHILE [:BAILEYISAWESOME = 0] [TELL [DICE7 DICE1 DICE5] ST BK 10 WAIT 100 COLLECT LINEDICE]
	IF :BAILEYISAWESOME = 1 [TELL "DICE7 SETX -498 SETY 183 TELL "DICE5 SETX -420 SETY 237 TELL "DICE1 SETX -655 SETY 312 MAKE "MERRITTISAWESOME 1]
	
	WHILE [:MERRITTISAWESOME = 1] [TELL [DICE25 DICE19 DICE8] ST BK 10 WAIT 100 COLLECT LINEDICE]
	IF :MERRITTISAWESOME = 2 [TELL "DICE25 SETX 359 SETY 265 TELL "DICE19 SETX 258 SETY 206 TELL "DICE8 SETX -145 SETY 306 MAKE "FRED 1]
	
	WHILE [:FRED = 1] [TELL [DICE2 DICE6 DICE10] ST BK 10 WAIT 100 COLLECT LINEDICE]
	IF :FRED = 2 [TELL "DICE2 SETX -533 SETY 285 TELL "DICE6 SETX -319 SETY 238 TELL "DICE10 SETX -402 SETY 288 MAKE "BOGEY 1]
	
	WHILE [:BOGEY = 1] [TELL [DICE24 DICE22 DICE20] ST BK 10 WAIT 100 COLLECT LINEDICE]
	IF :BOGEY = 2 [TELL "DICE24 SETX 650 SETY 231 TELL "DICE22 SETX 480 SETY 229 TELL "DICE20 SETX 37 SETY 188 MAKE "2CUTEFORU 1]
	
	
	WHILE [2CUTEFORU = 1] [TELL [DICE14 DICE21 DICE18] ST BK 10 WAIT 100 COLLECT LINEDICE]
	IF 2CUTEFORU = 2 [TELL "DICE14 SETX 373 SETY 201 TELL "DICE21 SETX 548 SETY 166 TELL "DICE18 SETX 204 SETY 161 MAKE "TURKEY 1]
	
	
	WHILE [:TURKEY = 1] [TELL [DICE3 DICE11 DICE12 DICE16] ST BK 10 WAIT 100 COLLECT LINEDICE]
	IF :TURKEY = 2 [TELL "DICE3 SETX 136 SETY 238 TELL "DICE11 SETX -529 SETY 220 TELL "DICE12 SETX 95 SETY 184 TELL "DICE16 SETX -95 SETY 162 MAKE "BOBS 1]
	
	
	WHILE [:BOBS = 1] [TELL [DICE4 DICE13 DICE15 DICE17 DICE23] ST BK 10 WAIT 100 COLLECT LINEDICE]
	IF :BOBS = 2 [TELL "DICE4 SETX -625 SETY 180 TELL "DICE13 SETX 220 SETY 231 TELL "DICE15 SETX 413 SETY 288 TELL "DICE17 SETX 279 SETY 290 TELL "DICE23 SETX 129 SETY 242 MAKE "ACE 1]
	
	WHILE [:ACE = 1] [TELL "ACECARD ST BK 10 WAIT 100 COLLECT LINEDICE]
	IF :ACE = 2 [TELL "ACECARD SETX 21 SETY 215 MAKE "BAILEYISAWESOME 0 FALL]
END

