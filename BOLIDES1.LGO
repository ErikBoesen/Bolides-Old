	IF :SPACESHIPDISTANCEASTEROID.3 < 100 [TELL "SPACESHIP SETXY [0 0] SETH 0 SETVELOCITY 0 MAKE "HEARTS :HEARTS - 1 ASTEROIDS]
	TELL "ASTEROID.4
